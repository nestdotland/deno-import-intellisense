{
  "version": 1,
  "registries": [
    {
      "schema": "/:module([a-z0-9_]*)@:version/:path*",
      "variables": [
        {
          "key": "module",
          "url": "https://intellisense.nest.land/x"
        },
        {
          "key": "version",
          "url": "https://intellisense.nest.land/x/${module}"
        },
        {
          "key": "path",
          "url": "https://intellisense.nest.land/x/${module}/${version}/paths"
        }
      ]
    }
  ]
}
